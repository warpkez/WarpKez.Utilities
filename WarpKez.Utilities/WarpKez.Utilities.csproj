    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageProjectUrl>https://github.com/warpkez/WarpKez.Utilities</PackageProjectUrl>
    <RepositoryUrl>https://github.com/warpkez/WarpKez.Utilities</RepositoryUrl>
    <Title>$(AssemblyName)</Title>
